{
  
  "manifest_version": 2,
  "name": "Track Price",
  "version": "1.0",
  "description": "Price tracker lets you compare prices of products from different sites and let you buy at the best   ",
  "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png" 
  },



  "content_scripts":[
        {
            "matches":    [ "https://www.amazon.co.uk/*"],
            "js": ["jquery-3.4.1.min.js", "content.js"],
            "css": ["content.css"]
        }
  ]
}
